{
  "headers": [
    {
      "source": "/api/auth/:path*",
      "headers": [
        {
          "key": "X-Robots-Tag",
          "value": "noindex"
        }
      ]
    }
  ],
  "security": {
    "challenge": {
      "bypass": [
        {
          "type": "header",
          "key": "User-Agent",
          "value": "com.apple.WebKit.Networking"
        }
      ]
    }
  },
  "rewrites": [
    {
      "source": "/((?!www\\.).*)",
      "destination": "https://www.seasoners.eu/$1",
      "has": [
        {
          "type": "host",
          "value": "seasoners\\.eu"
        }
      ]
    }
  ]
}
